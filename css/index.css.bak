*{
    box-sizing:border-box;
}
body{
    font:12px "microsoft yahei",Arial,Helvetica,sans-serif;
    color:#666;
}
body,h1,h2,h3,h4,h5,h6,p,ul,ol,dl,dd,a {
    margin: 0;
    padding: 0;
    list-style: none;
    text-decoration: none;
}
img {
    vertical-align:center;
}
body{
    background-image:url(../imgs/index/yq0KXFZvwymAFNRwAABiJ5LGjr8416 - 01.jpg);
    /*position:fixed;*/
    background-size:100%;
}
.lines{
    width:5em;
    border:0.2em solid white;
    margin:4% auto;
}
#header-tab>div>a{
    float:right;
    display:inline-block;
    text-align:center;
    vertical-align:middle;
    line-height:4em;
    padding:0 2%;
    text-decoration:none;
    font-size:1.2em;
    color:black;
    border-top:0.3em solid transparent;
}
#header-tab>div>a:hover{
    border-top:0.3em solid #59CAAA;
    color:#59CAAA;
}
#header-tab>div>strong{
    float:left;
    font-size:4em;
    color:black;
    text-decoration:none;
}
